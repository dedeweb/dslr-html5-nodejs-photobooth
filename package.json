{
  "name": "tamerbooth",
  "version": "1.0.0",
  "description": "A photobooth webapp using DSLR to take pictures with gphoto2",
  "main": "index.js",
  "devDependencies": {
    "body-parser": "~1.18.3",
    "child_process": "~1.0.2",
    "colors": "~1.3.0",
    "concurrently": "~3.5.1",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "~4.5.1",
    "express": "~4.16.3",
    "fs-extra": "~6.0.1",
    "moment": "~2.22.1",
    "morgan": "~1.9.0",
    "nedb": "~1.8.0",
    "pem": "~1.12.5",
    "printer": "~0.2.2",
    "process": "~0.11.9",
    "request": "~2.87.0",
    "serve-favicon": "~2.5.0",
    "sharp": "~0.20.2",
    "socket.io": "~2.1.1",
    "socket.io-p2p-server": "~1.2.0",
    "webpack": "~4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server" : "cd server && node ../node_modules/webpack-cli/bin/webpack",
    "start:server": "node server/app",
    "server": "node server/app",
    "camera": "concurrently --kill-others \"npm run server\" \"cd camera_capture && ng serve --proxy-config proxy.conf.json  --host 0.0.0.0\"",
    "start": "npm run build && node dist/app",
    "build": "cd admin && npm run build && cd ../camera_capture && npm run build && cd ../front && npm run build && cd ../server && npm run build",
    "install-all": "npm install && cd admin && npm install && cd ../camera_capture && npm install && cd ../front && npm install && cd ../common && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dedeweb/tamerbooth.git"
  },
  "keywords": [
    "photobooth",
    "booth",
    "angular2",
    "express",
    "node",
    "js",
    "webcam",
    "dslr",
    "gphoto2",
    "webapp"
  ],
