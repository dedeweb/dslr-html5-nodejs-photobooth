{
  "name": "tamerbooth",
  "version": "1.0.0",
  "description": "A photobooth webapp using DSLR to take pictures with gphoto2",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.5.1",
    "socket.io-p2p-server": "^1.2.0"
  },
  "devDependencies": {
    "jimp": "^0.2.27"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "node server/app",
    "camera": "concurrently --kill-others \"npm run server\" \"cd camera_capture && ng serve --proxy-config proxy.conf.json  --host 0.0.0.0\"",
    "start": "cd camera_capture && ng build",
    "postinstall": "cd admin && npm install && cd ../camera_capture && npm install && cd ../front && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dedeweb/tamerbooth.git"
  },
  "keywords": [
    "photobooth",
    "booth",
    "angular2",
    "express",
    "node",
    "js",
    "webcam",
    "dslr",
    "gphoto2",
    "webapp"
  ],
  "author": "Denis Messi√©",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dedeweb/tamerbooth/issues"
  },
  "homepage": "https://github.com/dedeweb/tamerbooth#readme"
}
